{
    "name": "Greynir Translate",
    "description": "Translate websites between English and Icelandic using velthyding.mideind.is",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "service_worker.js"
    },
    "permissions": [
        "contextMenus",
        "activeTab",
        "scripting",
        "storage"
    ],
    "action": {
        "default_icon": {
            "16": "/images/velthyding_logo_16.png",
            "32": "/images/velthyding_logo_32.png",
            "48": "/images/velthyding_logo_48.png",
            "128": "/images/velthyding_logo_128.png"
        }
    },
    "icons": {
        "16": "/images/velthyding_logo_16.png",
        "32": "/images/velthyding_logo_32.png",
        "48": "/images/velthyding_logo_48.png",
        "128": "/images/velthyding_logo_128.png"
    }
}